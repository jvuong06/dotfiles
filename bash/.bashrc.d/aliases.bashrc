# Aliases for ~/.bashrc

alias cfg-nvim="cd $HOME/.config/nvim"
alias cfg-hypr="cd $HOME/.config/hypr"
alias cfg-qs="cd $HOME/.config/quickshell"
alias edit-pbase="nvim $HOME/.local/package-lists/base-packages"
alias edit-paur="nvim $HOME/.local/package-lists/aur-packages"
alias edit-ibase="nvim $HOME/.local/bin/install-base"
alias edit-iaur="nvim $HOME/.local/bin/install-base"
